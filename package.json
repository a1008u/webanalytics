    "aws-sdk": "^2.417.0",
    "axios": "^0.18.0",
    "crypto-browserify": "^3.12.0",
    "crypto-js": "^3.1.9-1",
    "dotenv": "^8.0.0",
    "fs": "0.0.1-security",
    "jest-localstorage-mock": "^2.4.0",
