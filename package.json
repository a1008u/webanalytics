  "main": "test.js",
  "scripts": {
    "win_build_local_dev": "SET NODE_ENV=local_dev & npm-run-all clean cpx:file win_build_local_dev:*",
    "win_build_local_dev:dataly": "SET NODE_ENV=local_dev & webpack --config webpack/dataly/webpack.config.js",
    "win_build_local_dev:implementorjs": "SET NODE_ENV=local_dev & webpack --config webpack/implementorjs/webpack.config.js",
    "build_local_dev": "NODE_ENV=local_dev npm-run-all clean cpx:file build_local_dev:*",
    "build_local_dev:dataly": "NODE_ENV=local_dev webpack --config webpack/dataly/webpack.config.js",
    "build_local_dev:implementorjs": "NODE_ENV=local_dev webpack --config webpack/implementorjs/webpack.config.js",
    "build_dev": "npm-run-all clean build_dev:*",
    "build_dev:dataly": "NODE_ENV=dev webpack --config webpack/dataly/webpack.config.js",
    "build_dev:implementorjs": "NODE_ENV=dev webpack --config webpack/implementorjs/webpack.config.js",
    "clean": "rimraf public",
    "cpx:file": "cpx \"html/**/*.html\" ./public/html",
    "test": "jest --coverage --no-cache",
    "lint": "eslint src/** --ext .ts,.tsx",
    "lint:dataly": "eslint src/dataly/** --ext .ts,.tsx",
    "lint:implementorjs": "eslint src/implementorjs/** --ext .ts,.tsx",
    "lintfix": "eslint src/** --ext .ts,.tsx --fix",
    "lintfix:dataly": "eslint src/dataly/** --ext .ts,.tsx --fix",
    "lintfix:implementorjs": "eslint src/implementorjs/** --ext .ts,.tsx --fix",
    "lintfix:config": "eslint src/config/** --ext .ts,.tsx --fix"
  },
  "author": "",
  "license": "ISC",
